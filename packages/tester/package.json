{
  "name": "melpack-tester",
  "version": "1.0.36",
  "scripts": {
    "buildScript": "babel-node ./melpack/build.js",
    "testScript": "babel-node ./melpack/test.js",
    "build": "npm run buildScript -- --environment=development",
    "build:watch": "npm run buildScript -- --environment=development --watch",
    "build:production": "npm run buildScript -- --environment=production",
    "build:production:analyzer": "npm run build:production -- --analyzer",
    "build:qa": "npm run buildScript -- --environment=qa",
    "test": "npm run testScript -- --environment=test",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "melpack-module-builder": "^1.0.35"
  },
  "engines": {
    "node": ">6.0"
  },
  "author": "DadViegas",
  "repository": {
    "type": "git",
    "url": "https://github.com/dadviegas/melpack"
  }
}
