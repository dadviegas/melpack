  "repository": {
    "type": "git",
    "url": "https://github.com/dadviegas/melpack"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.0"
  }
}
