{
  "name": "melpack-module-tester",
  "version": "1.0.29",
  "scripts": {
    "buildScript": "babel-node ./melpack/build.js",
    "testScript": "babel-node ./melpack/test.js",
    "build": "npm run buildScript -- --environment=development",
    "build:production": "npm run buildScript -- --environment=production",
    "build:production:analyzer": "npm run build:production -- --analyzer",
    "build:qa": "npm run buildScript -- --environment=qa",
    "test": "npm run testScript -- --environment=test",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "melpack": "^1.0.28",
    "melpack-analyzer-middleware": "^1.0.22",
    "melpack-babel-middleware": "^1.0.21",
    "melpack-define-middleware": "^1.0.26",
    "melpack-entry-middleware": "^1.0.21",
    "melpack-optimize-middleware": "^1.0.21",
    "melpack-output-middleware": "^1.0.22"
  },
  "engines": {
    "node": ">6.0"
  },
  "author": "DadViegas",
  "repository": {
    "type": "git",
    "url": "https://github.com/dadviegas/melpack"
  }
}
